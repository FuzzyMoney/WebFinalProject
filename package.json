{
  "name": "react-express",
  "version": "0.0.2",
  "description": "A template for web apps using React and Express",
  "main": "app.js",
  "scripts": {
    "dev": "npm run dev-nodemon & npm run dev-webpack & npm run dev-browsersync",
    "dev-nodemon": "nodemon --exec babel-node app.js --ignore public/",
    "dev-webpack": "webpack -wd",
    "dev-browsersync": "browser-sync start --config browser-sync.js",
